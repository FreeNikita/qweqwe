{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "extends": ["airbnb"],
    "plugins": ["jest"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "rules": {
        "no-shadow": "off",
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off"
    }
}
